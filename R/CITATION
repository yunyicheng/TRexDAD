citEntry(
  entry    = "Unpublished",
  title    = "TRexDAD: Data-driven Pipeline for Tile Region Exchange Assembly Design",
  author   = "Yunyi Cheng",
  year     = 2023,
  note     = "Unpublished",
  textVersion =
  paste("Cheng, Y. (2023) TRexDAD: Data-driven Pipeline for Tile Region Exchange Assembly Design.
        Unpublished. URL https://github.com/yunyicheng/TRexDAD")
)

